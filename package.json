{
  "name": "rxjs-game",
  "description": "Game example build with rxjs",
  "version": "1.0.0",
  "scripts": {
    "build:prod": "NODE_ENV=development parcel build dist/index.html",
    "watch": "NODE_ENV=development parcel src/index.html --open",
    "test": "jest",
    "prettier": "prettier --write '**/*.{js,jsx}'",
    "lint": "eslint **/*.{js,jsx}",
    "size": "size-limit",
    "prepublishOnly": "npm run lint && npm run prettier && npm run build:prod"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/typical000/rxjs-game"
  },
  "author": "Pavel Davydov",
  "licence": "MIT",
  "dependencies": {
    "pixi.js": "^4.8.5",
    "rxjs": "^6.4.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.12.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.2.0",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "parcel-bundler": "^1.10.3",
    "parcel-plugin-typed-css-modules": "^1.1.0",
    "postcss-modules": "^1.4.1",
    "prettier": "^1.16.0",
    "size-limit": "^0.21.1"
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  }
}
